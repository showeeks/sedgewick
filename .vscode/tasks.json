{
    "version": "2.0.0",
    "tasks": [
        {
            "type": "shell",
            "label": "build list",
            "command": "/usr/bin/make",
            "args": [
                "target/list",
            ],
            "options": {
                "cwd": "${workspaceRoot}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": "build"
        },
        {
            "type": "shell",
            "label": "build matrix",
            "command": "/usr/bin/make",
            "args": [
                "target/matrix"
            ],
            "options": {
                "cwd": "${workspaceRoot}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": "build"
        },
        {
            "type": "shell",
            "label": "build union find",
            "command": "/usr/bin/make",
            "args": [
                "target/uf",
            ],
            "options": {
                "cwd": "${workspaceRoot}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": "build"
        }
    ]
}